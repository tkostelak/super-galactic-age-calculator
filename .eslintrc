{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jquery": true
    },
    "rules": {
        "no-console": "warn",
        "semi": 1,
        "indent": ["warn", 2],
        "no-useless-escape": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn"
    }
}
